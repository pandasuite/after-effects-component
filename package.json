{
  "name": "after-effects-component",
  "version": "2.0.1",
  "main": "index.js",
  "repository": "https://github.com/pandasuite/after-effects-component.git",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "rm -rf ./pandasuite-component.zip && webpack --mode production --devtool none && cd build && zip -r -X '../pandasuite-component.zip' *",
    "release": "yarn build && release-it --no-npm.publish"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^5.2.6",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "postcss": "^8.3.0",
    "postcss-loader": "4",
    "release-it": "^13.6.3",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "lottie-web": "^5.9.6",
    "pandasuite-bridge": "^4.5.0"
  },
  "release-it": {
    "github": {
      "release": true,
      "assets": [
        "pandasuite-component.zip"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "IE >= 9",
      "Android > 0",
      "iOS > 0"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
